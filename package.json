    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "socket.io": "^2.3.0",
    "use-socket.io-client": "^1.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/socket.io": "^2.1.8",
    "less": "^3.11.3",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.8.3"
